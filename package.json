{
  "name": "ogame",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate-openapi": "curl http://localhost:8080/api-docs | jq . > ../openapi.json",
    "generate-api": "npx swagger-typescript-api -p ../openapi.json -o src/api/generated --modular --clean-output --module-name-first-tag --sort-types --disable-throw-on-error --templates ./swagger-typescript-templates",
    "generate-api-live": "npx swagger-typescript-api -p http://localhost:8080/api-docs -o src/api/generated --modular --clean-output --module-name-first-tag --sort-types --disable-throw-on-error --templates ./swagger-typescript-templates"
  },
  "dependencies": {
    "@shoelace-style/shoelace": "^2.16.0",
    "eslint-config-love": "^62.0.0",
    "intl-messageformat": "^10.5.14",
    "lit": "^3.1.4",
    "mobx": "^6.13.1",
    "mobx-lit-element": "^0.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "swagger-typescript-api": "^13.0.14",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
